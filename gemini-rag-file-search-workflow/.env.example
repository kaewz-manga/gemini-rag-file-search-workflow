# n8n Configuration
N8N_BASE_URL=https://your-n8n-instance.com
N8N_WEBHOOK_PATH=/webhook/gemini-rag-file-search

# MCP Server Configuration
MCP_ENDPOINT_URL=https://your-mcp-server.com/mcp/gemini-rag

# Google Drive Configuration
GOOGLE_DRIVE_FOLDER_ID=your-google-drive-folder-id

# Data Table IDs
# สร้าง Data Tables ใน n8n ก่อน แล้วคัดลอก IDs มาใส่ที่นี่
DATA_TABLE_MAIN_ID=your-main-data-table-id        # Gemini File RAG
DATA_TABLE_DOCUMENT_ID=your-document-table-id     # Gemini File RAG: Page Document

# Gemini API (สำหรับ AI Agent)
# ตั้งค่าผ่าน n8n Credentials UI
# GEMINI_API_KEY=your-gemini-api-key

# Google Drive OAuth2
# ตั้งค่าผ่าน n8n Credentials UI
# GOOGLE_CLIENT_ID=your-client-id
# GOOGLE_CLIENT_SECRET=your-client-secret

# Webhook Configuration
WEBHOOK_ID=gemini-rag-webhook-001
WEBHOOK_METHOD=POST

# Optional: Timezone
TIMEZONE=Asia/Bangkok

# Optional: Execution Settings
EXECUTION_ORDER=v1
SAVE_DATA_ERROR_EXECUTION=none
SAVE_DATA_SUCCESS_EXECUTION=all
SAVE_MANUAL_EXECUTIONS=false
SAVE_EXECUTION_PROGRESS=false
