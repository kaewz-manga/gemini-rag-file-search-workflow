  "curl_examples": {
    "create_store": "curl -X POST 'https://{{N8N_BASE_URL}}/webhook/gemini-rag-file-search' \\\n  -H 'Content-Type: application/json' \\\n  -d '{\"operation\": \"Create Store\", \"displayName\": \"My Knowledge Base\"}'",
    "upload_store": "curl -X POST 'https://{{N8N_BASE_URL}}/webhook/gemini-rag-file-search' \\\n  -F 'operation=Upload Store' \\\n  -F 'displayName=Tech Docs' \\\n  -F 'category=documentation' \\\n  -F 'tags=[\"API\",\"REST\"]' \\\n  -F 'image=@/path/to/file.pdf'",
    "search": "curl -X POST 'https://{{N8N_BASE_URL}}/webhook/gemini-rag-file-search' \\\n  -H 'Content-Type: application/json' \\\n  -d '{\"operation\": \"Search Store\", \"file_search_store_name\": \"corpora/abc123\", \"text_input\": \"What is RAG?\"}'"
  },
  "notes": {
    "tags_format": [
      "JSON Array: '[\"tag1\", \"tag2\", \"tag3\"]'",
      "Comma-separated: 'tag1, tag2, tag3'",
      "Newline-separated: 'tag1\\ntag2\\ntag3'"
    ],
    "file_upload": "ใช้ multipart/form-data และส่งไฟล์ในฟิลด์ 'image'",
