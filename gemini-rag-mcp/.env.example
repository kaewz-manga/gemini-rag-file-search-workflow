# Gemini RAG File Search MCP - Environment Variables
# Copy to .env and fill in your values

# Cloudflare
CLOUDFLARE_ACCOUNT_ID=your_account_id
CLOUDFLARE_API_TOKEN=your_api_token

# Gemini API
GEMINI_API_KEY=your_gemini_api_key

# JWT Secret (generate: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
JWT_SECRET=your_jwt_secret_here

# Encryption Key (generate: node -e "console.log(require('crypto').randomBytes(32).toString('hex'))")
ENCRYPTION_KEY=your_encryption_key_here

# D1 Database ID
D1_DATABASE_ID=your_d1_database_id

# KV Namespace ID
KV_NAMESPACE_ID=your_kv_namespace_id
